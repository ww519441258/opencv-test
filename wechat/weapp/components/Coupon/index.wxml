<block wx:if="{{$taroCompReady}}"><view class="coupon"><view class="saw-tooth-bar"><view wx:key="index" class="tooth" wx:for="{{$anonymousCallee__1}}" wx:for-item="index" wx:for-index="_anonIdx2"></view></view><block><block wx:if="{{type === 1}}"><view class="coupon__left"><view class="number">￥{{favorable_content}}</view><block wx:if="{{least_cost}}"><view class="desc">满{{least_cost}}元可用</view></block></view></block><block wx:elif="{{type === 2}}"><view class="coupon__left"><view class="number"><text>{{(100 - favorable_content) / 10}}</text><text>折</text></view><block wx:if="{{least_cost}}"><view class="desc">满{{least_cost}}元可用</view></block></view></block></block><view class="coupon__right"><view class="gap-group"><view class="gap__left"></view><view class="gap__right"></view></view><view class="main"><view class="name">{{title}}</view><view class="date"><block><block wx:if="{{useableTimeBegin}}"><text>{{anonymousState__temp}} 至 {{anonymousState__temp2}}</text></block><block wx:else><block><block wx:if="{{fixed_begin_term > 0}}"><text style="{{anonymousState__temp3}}">领取{{fixed_begin_term}}天后有效</text></block><block wx:if="{{fixed_term > 0}}"><text>{{fixed_begin_term <= 0 ? '领取后' : ''}}有效期{{fixed_term}}天</text></block></block></block></block></view><view class="desc">{{description}}</view></view><block wx:if="{{mode !== 'display'}}"><view class="coupon__btn"><view class="btn" bindtap="handleClickCoupon">{{mode === 'take' ? '领取' : '使用'}}</view></view></block></view></view></block>
