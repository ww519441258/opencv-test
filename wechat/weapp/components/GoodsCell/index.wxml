<block wx:if="{{$taroCompReady}}"><view class="{{'goods-cell ' + (!disabled && 'canClick')}}" bindtap="gotoGoodsDetail"><view class="goods-cell--image"><image src="{{goods.goods_img}}" mode="aspectFit"></image></view><view class="goods-cell--info"><view class="goods-cell--name">{{goods.goods_name}}</view><view class="row"><view class="left"><block wx:if="{{goods.sku_type}}"><view class="goods-cell--sku">{{anonymousState__temp}}</view></block><view class="goods-cell--data"><block><block wx:if="{{isExchange}}"><price compid="{{$compid__132}}"></price></block><block wx:else><price compid="{{$compid__133}}"></price></block></block></view></view><block wx:if="{{goods.current_group_size !== undefined}}"><view>{{goods.current_group_size}}人已拼</view></block><block wx:if="{{mainButton}}"><view class="right"><button type="primary" size="mini" hover-stop-propagation="{{true}}" catchtap="onClickMain" disabled="{{disableMainButton}}">{{mainButton}}</button></view></block></view></view><view class="goods-cell--number"><block wx:if="{{goods.goods_number}}"><text>x {{goods.goods_number}}</text></block></view><block wx:if="{{goods.qrCode && goods.qrCode.length > 0}}"><button class="qr-code iconfont icon-qrcode" plain="{{true}}" hover-stop-propagation="{{true}}" catchtap="handleClickQrCode"></button></block></view></block>
